{% highlight ruby %}
module Ex25
  def self.break_words(stuff)
    words=stuff.split(' ')
    words
  end

  def self.sort_words(words)
    words.sort()
  end
 
  def self.print(word)
    puts word
  end
end
{% endhighlight %}
